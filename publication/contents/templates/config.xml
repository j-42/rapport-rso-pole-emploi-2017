<?xml version="1.0" encoding="UTF-8"?><root>
<configs>
<modules>
<pdf mergeservice="$managerHost$/services/rest/playerGateway/pdfmerge"/>
<email service="$managerHost$/services/rest/playerGateway/sendmail"/>
</modules>
<scrollBar>
<general useArrows="false"/>
</scrollBar>
<thumbnails>
<mode value="newSkin"/>
</thumbnails>
<gallery>
<padding left="50" top="20"/>
<align value="tl"/>
<item gapColumn="40" gapLine="60" height="150" ignoreBack="true" newRenderer="true" width="150"/>
<exclude>
<item id="details"/>
<item id="checkbox"/>
<item id="download"/>
</exclude>
</gallery>
<viewerType value="2d"/>
<search>
<autoClose value="true"/>
</search>
<menubutton>
<brightness value="-0.5"/>
</menubutton>
<summary>
<autoClose value="true"/>
</summary>
<bookmarks lib_id="main_lib" symbol_id="pagemark"/>
<deepLinking value="true"/>
<driveScrollbar>
<general style="driveScrollbar"/>
</driveScrollbar>
<flipBook>
<sound enable="true"/>
<keyControl enabled="true"/>
<leftGradient alphas="0,0.02,0.15" colors="0,0,0" ratio="0x00,0x9f,0xff" w="50"/>
<rightGradient alphas="0.2,0.05,0" colors="0,0,0" ratio="0x00,0x9f,0xff" w="50"/>
<flip canceldist="0.5" cornerdist="0.25" curveheight="0" tweendelay="0.2"/>
<miniflip delay="1" destx="100" desty="20" speed="1" startx="30" starty="6"/>
<pubChange resetToFirstPage="false" transitionTime="0.3"/>
<lights>
<directional ambient="0.3" color="0xFFFFFF" diffuse="0.2" direction="-1,0,1" specular="0.2"/>
<directional ambient="0.3" color="0xFFFFFF" diffuse="0.2" direction="1,0,1" specular="0.2"/>
<point ambient="0.1" color="0xFFFFFF" diffuse="1" specular="0" x="500" z="-1000"/>
</lights>
</flipBook>
<pageLayoutHTML value="best"/>
</configs>
</root>
